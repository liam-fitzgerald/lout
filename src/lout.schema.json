{
  "$schema": "http://json-schema.org/schema",
  "type": "object",
  "properties": {
    "version": {
      "type": "number"
    },
    "dependencies": {
      "type": "object",
      "patternProperties": {
        "^(\\/[a-z]+)+$": {
          "type": "string"
        }
      }
    }
  },
  "required": ["dependencies", "version"]
}
